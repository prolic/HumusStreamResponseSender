{
    "name": "prolic/humus-stream-response-sender",
    "type": "library",
    "description": "ZF2 module that can send stream responses with http range header, xsendfile & pecl_http support",
    "keywords": ["zf2", "stream", "humus", "http", "nginx" , "apache" , "xsendfile"],
    "homepage": "https://github.com/prolic/HumusStreamResponseSender",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "zendframework/zend-mvc": "^2.2.1|^3.0",
        "zendframework/zend-http": "^2.2.1|^3.0",
        "zendframework/zend-modulemanager": "^2.2.1|^3.|^3.0"
    },
    "require-dev": {
        "zendframework/zendframework": "^2.2.1",
        "phpunit/phpunit": "^5.7|^6.0",
        "phpmd/phpmd": "1.5.*",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6"
    },
	"extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "HumusStreamResponseSender\\": "src"
        }
    }
}
